<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.strikkeapp.strikkeapp.controllers.loginControllers.RegisterController">
   <children>
      <Label layoutX="126.0" layoutY="29.0" text="Resgister">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="16.0" layoutY="98.0" prefHeight="35.0" prefWidth="320.0" promptText="Enter Username" />
      <TextField fx:id="emailField" layoutX="15.0" layoutY="223.0" prefHeight="35.0" prefWidth="320.0" promptText="Enter email" />
      <TextField fx:id="firstnameField" layoutX="16.0" layoutY="283.0" prefHeight="35.0" prefWidth="320.0" promptText="Enter First name" />
      <TextField fx:id="lastnameField" layoutX="15.0" layoutY="343.0" prefHeight="35.0" prefWidth="320.0" promptText="Enter Last name" />
      <PasswordField fx:id="passwordField" layoutX="15.0" layoutY="159.0" prefHeight="35.0" prefWidth="320.0" promptText="Enter Password" />
      <Button layoutX="15.0" layoutY="403.0" mnemonicParsing="false" onAction="#setRegisterButtonAction" prefHeight="35.0" prefWidth="320.0" text="Register">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Hyperlink fx:id="loginHyperlink" layoutX="198.0" layoutY="468.0" onAction="#setLoginHyperlinkAction" prefHeight="16.0" prefWidth="59.0" text="Log in" underline="true" />
      <Label layoutX="95.0" layoutY="471.0" text="Already registered?" />
   </children>
</AnchorPane>
